{
    "name": "lpedretti/ckeditor",
    "description": "CKEditor plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CKEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CKEditor\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
